:root{
--mdc-theme-primary: #4dd0e1 !important;
--mdc-theme-primary-light: #88ffff !important;
--mdc-theme-primary-dark: #009faf !important;
--mdc-theme-secondary: #263238 !important;
--mdc-theme-secondary-light: #4f5b62 !important;
--mdc-theme-secondary-dark: #000a12 !important;
--mdc-theme-background: #fff !important !important;
--mdc-theme-text-primary-on-primary: #000000 !important;
--mdc-theme-text-primary-on-primary-dark: #000000 !important;
--mdc-theme-text-primary-on-secondary: #ffffff !important;
--mdc-theme-text-primary-on-secondary-dark: #ffffff !important;
--mdc-theme-text-primary-on-dark: #fff !important !important;
--mdc-theme-text-secondary-on-primary: hsla(0,0%,100%,.7) !important !important;
--mdc-theme-text-hint-on-primary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-disabled-on-primary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-icon-on-primary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-primary-on-primary-light: rgba(0,0,0,.87) !important !important;
--mdc-theme-text-secondary-on-primary-light: rgba(0,0,0,.54) !important !important;
--mdc-theme-text-hint-on-primary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-disabled-on-primary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-icon-on-primary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-secondary-on-primary-dark: hsla(0,0%,100%,.7) !important !important;
--mdc-theme-text-hint-on-primary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-disabled-on-primary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-icon-on-primary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-secondary-on-secondary: hsla(0,0%,100%,.7) !important !important;
--mdc-theme-text-hint-on-secondary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-disabled-on-secondary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-icon-on-secondary: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-primary-on-secondary-light: rgba(0,0,0,.87) !important !important;
--mdc-theme-text-secondary-on-secondary-light: rgba(0,0,0,.54) !important !important;
--mdc-theme-text-hint-on-secondary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-disabled-on-secondary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-icon-on-secondary-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-secondary-on-secondary-dark: hsla(0,0%,100%,.7) !important !important;
--mdc-theme-text-hint-on-secondary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-disabled-on-secondary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-icon-on-secondary-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-primary-on-background: rgba(0,0,0,.87) !important !important;
--mdc-theme-text-secondary-on-background: rgba(0,0,0,.54) !important !important;
--mdc-theme-text-hint-on-background: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-disabled-on-background: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-icon-on-background: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-primary-on-light: rgba(0,0,0,.87) !important !important;
--mdc-theme-text-secondary-on-light: rgba(0,0,0,.54) !important !important;
--mdc-theme-text-hint-on-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-disabled-on-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-icon-on-light: rgba(0,0,0,.38) !important !important;
--mdc-theme-text-secondary-on-dark: hsla(0,0%,100%,.7) !important !important;
--mdc-theme-text-hint-on-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-disabled-on-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-text-icon-on-dark: hsla(0,0%,100%,.5) !important !important;
--mdc-theme-surface: #fff !important !important;
--mdc-theme-error: #b00020 !important !important;
--mdc-theme-on-primary: #000000 !important;
--mdc-theme-on-secondary: #ffffff !important;
--mdc-theme-on-surface: #000 !important !important;
--mdc-theme-on-error: #fff !important !important;

--var-theme-primary: 77, 208, 225 !important;
--var-theme-secondary: 38, 50, 56 !important;
--var-theme-primary-light: 136, 255, 255 !important;
--var-theme-secondary-light: 79, 91, 98 !important;
--var-theme-primary-dark: 0, 159, 175 !important;
--var-theme-secondary-dark: 0, 10, 18 !important;

}



.mdc-icon-toggle--primary:after, .mdc-icon-toggle--primary:before {
    background-color: rgba(var(--var-theme-primary), 0.14) !important;
    opacity: 0 !important;
}

//.mdc-icon-toggle--secondary:after, .mdc-icon-toggle--secondary:before {
//    background-color: rgba(var(--var-theme-secondary), 0.14) !important;
//    opacity: 0 !important;
//}


.mdc-button:after, .mdc-button:before {
    background-color: rgba(var(--var-theme-primary), 0.16) !important;
    opacity: 0 !important;
}


.mdc-text-field--focused:not(.mdc-text-field--disabled) .mdc-floating-label {
    color: rgba(var(--var-theme-secondary), 1) !important;
}


::placeholder {
    color:  rgba(var(--var-theme-secondary), 1) !important;
}



::-webkit-scrollbar {
    display: none;
}


.mdc-list-item--selected .mdc-list-item__graphic,
.mdc-list-item--activated .mdc-list-item__graphic,
.mdc-list-item--selected, .mdc-list-item--activated {
    color: var(--mdc-theme-secondary, 1) !important;
}


 
